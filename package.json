{
  "name": "@senzing/rest-api-client-ng",
  "version": "0.0.4",
  "description": "A package providing restful http interfaces to typescript interfaces to @senzing/rest-api-server-java",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "build:docs": "npx compodoc -p projects/rest-api-client-ng/tsconfig.lib.json -n \"Senzing Rest Api Client (Angular)\" -d docs --disableInternal --disablePrivate --disableDomTree --disableGraph --disableRoutesGraph",
    "docs": "npm run build:docs",
    "clean": "rimraf dist/@senzing/rest-api-client-ng",
    "package": "npm run build && npm run build:docs && node post-build.js && cd ./dist/@senzing/rest-api-client-ng/ && npm pack",
    "publish": "cd ./dist/@senzing/rest-api-client-ng/ && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Senzing/rest-api-client-ng.git"
  },
  "keywords": [
    "REST",
    "OAS3",
    "OAS",
    "Senzing",
    "ER",
    "Entity Resolution"
  ],
  "author": "Ara Winters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Senzing/rest-api-client-ng/issues"
  },
  "homepage": "https://github.com/Senzing/rest-api-client-ng#readme",
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/jest": "^23.3.12",
    "@types/mocha": "^5.2.5",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@compodoc/compodoc": "^1.1.7",
    "@types/node": "~8.9.4",
    "ng-packagr": "^4.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
